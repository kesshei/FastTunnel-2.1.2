{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Yarp.ReverseProxy": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:1234",
  "EnableFileLog": false,
  "TelConfig": {
    "WebDomain": "test.cc",
    "EnableForward": true,
    "Tokens": [ "TOKEN_FOR_CLIENT_AUTHENTICATION" ]
  }
}
