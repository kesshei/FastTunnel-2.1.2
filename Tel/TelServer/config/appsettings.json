{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Yarp.ReverseProxy": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:1234",
  "EnableFileLog": false,
  "TelConfig": {
    "WebDomain": "",
    "EnableForward": true,
    "Tokens": [ "锄禾日当午，汗滴禾下土，谁知盘中餐，粒粒皆辛苦" ]
  }
}
